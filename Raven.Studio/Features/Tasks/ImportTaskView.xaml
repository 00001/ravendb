<UserControl x:Class="Raven.Studio.Features.Tasks.ImportTaskView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:tk="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit"
			 mc:Ignorable="d"
			 d:DesignHeight="300"
			 d:DesignWidth="400">

	<tk:DockPanel x:Name="LayoutRoot"
				  Background="{StaticResource DefaultScreenBackground}">
		<TextBlock Text="Import" tk:DockPanel.Dock="Top"
				   Style="{StaticResource PanelTitle}" />
		<StackPanel Orientation="Horizontal">
			<Button x:Name="ImportData"
					Content="Import" />
		</StackPanel>
	</tk:DockPanel>
</UserControl>
