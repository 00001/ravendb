<Infrastructure:View x:Class="Raven.Studio.Views.Logs"
					 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					 xmlns:Infrastructure="clr-namespace:Raven.Studio.Infrastructure"
                     xmlns:Logs="clr-namespace:Raven.Studio.Features.Logs"
                     DataContext="{Binding Source={StaticResource LogsModelLocator}}">

	<Grid>
		<Logs:LogsView DataContext="{Binding Path=Current.Value}" />
	</Grid>
</Infrastructure:View>